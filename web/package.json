{
  "name": "web",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "node scripts/dev-smart.mjs",
    "dev:raw": "next dev -p 3000",
    "dev:patched": "node scripts/patch-next-runtime-dev.mjs && next dev -p 3000",
    "build": "next build && node scripts/patch-next-runtime.mjs",
    "start": "next start",
    "start:patched": "node scripts/patch-next-runtime.mjs && next start -p 3030",
    "start:prod": "next start -p 3030",
    "preview": "npm run build && npm run start:prod",
    "build:staging": "dotenv -e .env.staging -- cross-env NODE_ENV=production next build",
    "start:staging": "dotenv -e .env.staging -- cross-env NODE_ENV=production next start -p 3031",
    "preview:staging": "npm run build:staging && npm run start:staging",
    "lint": "eslint",
    "migrate-db": "tsx src/scripts/migrate-to-db.ts"
  },
  "dependencies": {
    "@libsql/client": "^0.15.12",
    "@mozilla/readability": "^0.6.0",
    "@sentry/nextjs": "^10.5.0",
    "@sparticuz/chromium-min": "^119.0.0",
    "@tanstack/react-query": "5.85.5",
    "dotenv": "^17.2.1",
    "ffmpeg-static": "^5.2.0",
    "file-type": "^18.7.0",
    "fluent-ffmpeg": "^2.1.3",
    "ffprobe-static": "^3.1.0",
    "jsdom": "^26.1.0",
    "next": "15.5.0",
    "nodemailer": "^7.0.5",
    "puppeteer-core": "^22.13.1",
    "react": "19.1.0",
    "react-dom": "19.1.0",
    "sharp": "^0.33.5",
    "zod": "^3.23.8"
  },
  "devDependencies": {
    "@eslint/eslintrc": "^3.3.0",
    "@types/fluent-ffmpeg": "^2.1.25",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^20.14.11",
    "@types/react": "^19.1.0",
    "@types/react-dom": "^19.1.2",
    "autoprefixer": "^10.4.20",
    "cross-env": "^7.0.3",
    "dotenv-cli": "^7.4.4",
    "eslint": "^9.17.0",
    "eslint-config-next": "^15.1.4",
    "postcss": "^8.4.38",
    "tailwindcss": "^3.4.0",
    "tsx": "^4.19.2",
    "typescript": "^5.7.3"
  }
}
