{
  "framework": "nextjs",
  "buildCommand": "npm run build",
  "devCommand": "npm run dev",
  "installCommand": "npm install",
  "outputDirectory": ".next",
  "crons": [
    {
      "path": "/api/cron/feed-check",
      "schedule": "0 3 * * *"
    },
    {
      "path": "/api/cron/trending",
      "schedule": "0 * * * *"
    },
    {
      "path": "/api/cron/promote",
      "schedule": "30 2 * * *"
    },
    {
      "path": "/api/cron/social-media",
      "schedule": "0 4 * * *"
    }
  ]
}
